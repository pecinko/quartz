<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog">
    <defaultcontrol always="true">10</defaultcontrol>
    <include>DialogOpenClose</include>
    <controls>
        <control type="image">
            <centerleft>50%</centerleft>
            <centertop>50%</centertop>
            <width>988</width>
            <height>598</height>
            <texture>texture/dialog.png</texture>
            <bordertexture border="22">texture/dialog_border.png</bordertexture>
            <bordersize>22</bordersize>
        </control>
        <control type="group">
            <posx>480</posx>
            <posy>360</posy>
            <control type="label" id="1" description="Heading">
                <posy>-30</posy>
                <width>960</width>
                <label>-</label>
                <font>size28</font>
                <textcolor>HL</textcolor>
                <align>center</align>
            </control>
            <control type="label" id="2" description="Line 1">
                <posy>75</posy>
                <width>960</width>
                <align>center</align>
                <label>-</label>
            </control>
            <control type="label" id="3" description="Line 2">
                <posy>120</posy>
                <width>960</width>
                <align>center</align>
                <label>-</label>
            </control>
            <control type="label" id="4" description="Line 3">
                <posy>165</posy>
                <width>960</width>
                <align>center</align>
                <label>-</label>
            </control>
            <control type="progress" description="Progressbar">
                <visible>Window.IsActive(filemanager)</visible>
                <posx>0</posx>
                <posy>261</posy>
                <width>960</width>
                <height>9</height>
                <texturebg border="5,0,5,0">progress/ProgressBG.png</texturebg>
                <midtexture border="0,1,0,1">progress/ProgressMidd.png</midtexture>
                <righttexture border="1,1,5,1">progress/ProgressRight.png</righttexture>
                <lefttexture border="5,1,1,1">progress/ProgressLeft.png</lefttexture>
                <overlaytexture>-</overlaytexture>
                <info>System.Progressbar</info>
            </control>
            <control type="button" id="10" description="Cancel">
                <posx>330</posx>
                <posy>330</posy>
                <width>300</width>
                <label>222</label>
                <align>center</align>
            </control>
        </control>
    </controls>
</window>