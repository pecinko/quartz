<?xml version="1.0" encoding="UTF-8"?>
<includes>
  <include name="View_50" description="50">
    <control type="group" description="List 50 base">
      <visible>Control.IsVisible(50)</visible>
      <include>DefaultThumbs</include>
      <control type="group" description="Slider and List">
        <include>SlimListItemCount</include>
        <posx>1020</posx>
        <posy>141</posy>
        <include>SlimListSlider</include>
        <control type="fixedlist" id="50">
          <posx>0</posx>
          <posy>0</posy>
          <width>802</width>
          <height>891</height>
          <pagecontrol>60</pagecontrol>
					<onleft condition="!Skin.HasSetting(ViewOptions_Hide)">9000</onleft>
          <onright condition="Skin.HasSetting(Scrollbars_Active)">9001</onright>
          <onup>50</onup>
          <ondown>50</ondown>
          <viewtype label="535">List</viewtype>
          <focusposition>5</focusposition>
          <movement>5</movement>
          <preloaditems>4</preloaditems>
          <itemlayout condition="SubString(Container.FolderPath,plugin://,left)" width="802" height="81">
            <include>Watched_Marks</include>
            <control type="label" description="Main Label">
              <width>720</width>
              <include>52MainLabel</include>
              <label>$INFO[ListItem.Label]</label>
            </control>
          </itemlayout>
          <focusedlayout condition="SubString(Container.FolderPath,plugin://,left)" width="802" height="81">
            <include>Watched_Marks</include>
            <control type="label" description="Main Label">
              <width>720</width>
              <include>52MainLabelFO</include>
              <label>$INFO[ListItem.Label]</label>
            </control>
          </focusedlayout>
          <itemlayout width="802" height="81">
            <include>Watched_Marks</include>
            <!--<include>HD_Badge</include>-->
            <include>3D_Badge</include>
            <include>ArrowNF</include>
            <control type="label" description="Main Label">
              <visible>IntegerGreaterThan(ListItem.VideoResolution,700) | ListItem.IsFolder</visible>
              <include>52MainLabel</include>
              <label>$INFO[ListItem.Label]</label>
            </control>
            <control type="label" description="Main Label">
              <visible>![IntegerGreaterThan(ListItem.VideoResolution,700) | ListItem.IsFolder]</visible>
              <width>711</width>
              <include>52MainLabel</include>
              <label>$INFO[ListItem.Label]</label>
            </control>
            <control type="label" description="Label2">
              <visible>container.content(songs) | container.content(musicvideos)</visible>
              <include>52Label2</include>
              <label>$INFO[ListItem.Duration]</label>
            </control>
          </itemlayout>
          <focusedlayout width="802" height="81">
            <include>Watched_Marks</include>
            <!--<include>HD_Badge</include>-->
            <include>3D_Badge</include>
            <include>ArrowFO</include>
            <control type="label" description="Main Label">
              <visible>IntegerGreaterThan(ListItem.VideoResolution,700) | ListItem.IsFolder</visible>
              <include>52MainLabelFO</include>
              <label>$INFO[ListItem.Label]</label>
            </control>
            <control type="label" description="Main Label">
              <visible>![IntegerGreaterThan(ListItem.VideoResolution,700) | ListItem.IsFolder]</visible>
              <width>711</width>
              <include>52MainLabelFO</include>
              <label>$INFO[ListItem.Label]</label>
            </control>
            <control type="label" description="Label2">
              <visible>container.content(songs) | container.content(musicvideos)</visible>
              <include>52Label2FO</include>
              <label>$INFO[ListItem.Duration]</label>
            </control>
          </focusedlayout>
        </control>
      </control>
      <!--<include>SlimListFade</include>-->
